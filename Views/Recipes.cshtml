@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	Layout = "Master.cshtml";
}

@* the fun starts here *@

<main>
    <!--Main layout-->
    <div class="container">
        <div class="row">

            <!--Main column-->
            <div class="col-lg-8">
                @Html.Partial("ListLatestItemByCategory")
            </div>

            <!--Sidebar-->
            <div class="col-lg-4">

                <div class="widget-wrapper wow fadeIn" data-wow-delay="0.4s">
                    @Html.Partial("RecipeCategory")
                </div>

            </div>
            <!--/.Sidebar-->
        </div>
    </div>
    <!--/.Main layout-->

</main>

